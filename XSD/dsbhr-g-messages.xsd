	   targetNamespace="http://www.broservices.nl/xsd/dsbhr-g/1.0" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0.0">
	<xs:annotation>
		<xs:appinfo source="http://schemas.opengis.net/gmlsfProfile/2.0/gmlsfLevels.xsd">
			<gmlsf:ComplianceLevel>0</gmlsf:ComplianceLevel>
		</xs:appinfo>
	</xs:annotation>
	<xs:import namespace="http://www.broservices.nl/xsd/brocommon/3.0" schemaLocation="brocommon.xsd"/>
	<xs:import namespace="http://www.broservices.nl/xsd/bhrgcommon/1.0" schemaLocation="bhrgcommon.xsd"/>
