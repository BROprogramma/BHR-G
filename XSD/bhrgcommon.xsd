						</xs:annotation>
					</xs:element>
					<xs:element name="discontinuityType" type="bhrgcommon:DiscontinuityTypeType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:appinfo>
								<CodeListName>DiscontinuityType</CodeListName>
